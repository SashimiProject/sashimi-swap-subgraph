{
  "name": "uniswap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/SashimiProject/sashimi-swap-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create sashimi/binance --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy sashimi/binance --debug --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "deploy": "graph deploy sashimi/binance --ipfs http://18.177.152.38:5001 --node http://18.177.152.38:8020 --debug",
    "deploy-staging": "graph deploy sashimi/binance /Uniswap --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "deploy:bsc": "graph deploy sashimiproject/swapbsc --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "watch-local": "graph deploy sashimi/binance --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
